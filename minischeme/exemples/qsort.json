(begin

    (define qsort (lambda (xs)
      (if (< (count xs) 2)
        xs
        (append (qsort (filter (lambda (x) (< x (head xs))) (tail xs)))
                   (list (head xs))
                   (qsort (filter (lambda (x) (not (< x (head xs)))) (tail xs)))))))
  
    (qsort (list 5 4 2 3 1)))
  